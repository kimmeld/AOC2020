CALL S.GET.INPUT(2020,1,INPUT.LINES)
INPUT.LINES.COUNT = DCOUNT(INPUT.LINES,@FM)

* Part 1
* Find the two entries that add to 2020
FOR X = 1 TO INPUT.LINES.COUNT
  FOR Y = X+1 TO INPUT.LINES.COUNT
    IF INPUT.LINES<X> + INPUT.LINES<Y> = 2020 THEN
      CRT 'Found entries ':INPUT.LINES<X>:' and ':INPUT.LINES<Y>
      CRT 'PART 1 = ':INPUT.LINES<X>*INPUT.LINES<Y>
    END
  NEXT
NEXT

* Part 2 - same thing, but with three entries
FOR X = 1 TO INPUT.LINES.COUNT
  FOR Y = X+1 TO INPUT.LINES.COUNT
    FOR Z = Y+1 TO INPUT.LINES.COUNT
      IF INPUT.LINES<X> + INPUT.LINES<Y> + INPUT.LINES<Z> = 2020 THEN
        CRT 'Found entries ':INPUT.LINES<X>:', ':INPUT.LINES<Y>:', ':INPUT.LINES<Z>
        CRT 'PART 1 = ':INPUT.LINES<X>*INPUT.LINES<Y>*INPUT.LINES<Z>
      END
    NEXT
  NEXT
NEXT
