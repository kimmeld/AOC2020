CALL S.GET.INPUT(2020,18,INPUT.LINES)
INPUT.LINES.COUNT = DCOUNT(INPUT.LINES,@FM)

* Part 1
TOTAL = 0
FOR X = 1 TO INPUT.LINES.COUNT
  THIS.EVAL = 0
  CALL AOC.2020.18.EVAL(THIS.EVAL, INPUT.LINES<X>)
  TOTAL += THIS.EVAL
NEXT X
CRT 'Part 1 = ':TOTAL
 
* Part 2
TOTAL = 0
FOR X = 1 TO INPUT.LINES.COUNT
  THIS.EVAL = 0
  CALL AOC.2020.18.EVAL.PART2(THIS.EVAL, INPUT.LINES<X>)
  TOTAL += THIS.EVAL
NEXT X
CRT 'Part 2 = ':TOTAL
 
